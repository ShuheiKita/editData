<web-app>

	<filter>
		<filter-name>EncodeFilter</filter-name>
		<filter-class>filter.EncodeFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>EncodeFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>GetData</servlet-name>
		<servlet-class>dlsite.GetData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetData</servlet-name>
		<url-pattern>/GetData</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetBarData</servlet-name>
		<servlet-class>getData.GetBarData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetBarData</servlet-name>
		<url-pattern>/GetBarData</url-pattern>
	</servlet-mapping>
</web-app>